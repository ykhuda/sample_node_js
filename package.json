  "scripts": {
    "start": "nodemon --trace-warnings --inspect --experimental-modules --es-module-specifier-resolution=node ./bin/www.js",
    "db:migration:dev": "sequelize  db:migrate --env development",
    "db:migration:revert:dev": "sequelize  db:migrate:undo:all --env development",
    "db:seed:dev": "sequelize  db:seed:all --env development",
    "db:seed:revert:dev": "sequelize  db:seed:undo:all --env development",
    "db:migration:prod": "sequelize  db:migrate --env production",
    "db:migration:revert:prod": "sequelize  db:migrate:undo:all --env production",
    "db:seed:prod": "sequelize  db:seed:all --env production",
    "db:seed:revert:prod": "sequelize  db:seed:undo:all --env production"
  },
